{
  "name": "omlet-web-arcade",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:css && npm run serve",
    "serve": "http-server ./web -o -c-1",
    "build:css": "tailwindcss -i ./web/css/main.css -o ./web/css/tailwind.css --minify",
    "watch:css": "tailwindcss -i ./web/css/main.css -o ./web/css/tailwind.css --watch",
    "cap:add": "npx cap add android",
    "cap:open": "npx cap open android",
    "cap:copy": "npm run build:css && npm run build:overlay && npx cap copy android",
    "build:overlay": "esbuild web/js/floating_content.js --bundle --outfile=web/js/floating_content.bundle.js",
    "build:apk": "npm run cap:copy && cd android && .\\gradlew.bat assembleDebug",
    "install:device": "adb install ./android/app/build/outputs/apk/debug/app-debug.apk",
    "log:start": "adb logcat -s Capacitor WebView App System.err"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "autoprefixer": "^10.0.0",
    "esbuild": "^0.25.11",
    "http-server": "^14.1.1",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@capacitor/toast": "^7.0.2",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4"
  }
}
