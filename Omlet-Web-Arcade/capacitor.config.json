{
  "appId": "com.omletwebfinal",
  "appName": "Omlet Web Arcade",
  "webDir": "web",
  "server": {
    "androidScheme": "https",
    "allowNavigation": [
      "lh3.googleusercontent.com"
    ]
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": true,
      "backgroundColor": "#000000",
      "androidScaleType": "CENTER_CROP",
      "splashImmersive": true
    },
    "GameDetector": {
      "android": {
        "packageName": "com.omletwebfinal.plugins.gamedetector"
      }
    },
    "GoogleAuth": {
      "scopes": [
        "profile",
        "email"
      ],
      "serverClientId": "T917126689157-ngl5kda1rc4s7g6v85nue90ojo2sasj3.apps.googleusercontent.com",
      "forceCodeForRefreshToken": true
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound"
      ]
    }
  }
}